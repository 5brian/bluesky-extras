{
  "manifest_version": 3,
  "name": "bluesky-extras",
  "version": "1.1.0",
  "description": "display DID tools in bsky.app profile's context menu",
  "permissions": ["clipboardWrite"],
  "content_scripts": [
    {
      "matches": ["https://bsky.app/*"],
      "js": ["dist/content.js"]
    }
  ]
}
