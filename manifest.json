{
  "manifest_version": 3,
  "name": "bluesky-likes",
  "version": "1.0",
  "description": "beta",
  "permissions": ["activeTab"],
  "host_permissions": ["https://bsky.app/*", "https://bsky.social/*"],
  "content_scripts": [
    {
      "matches": ["https://bsky.app/profile/*"],
      "js": ["content.js"]
    }
  ]
}
