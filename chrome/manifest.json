{
  "manifest_version": 3,
  "name": "bluesky-extras",
  "version": "1.0.0",
  "description": "display DID tools in footer",
  "host_permissions": ["https://bsky.app/*", "https://bsky.social/*"],
  "content_scripts": [
    {
      "matches": ["https://bsky.app/*"],
      "js": ["dist/content.js"]
    }
  ]
}
