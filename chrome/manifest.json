{
  "manifest_version": 3,
  "name": "bluesky-likes",
  "version": "1.0.0",
  "description": "display users likes",
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://bsky.app/*",
    "https://bsky.social/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://bsky.app/*"],
      "js": ["dist/content.js"]
    }
  ]
}
